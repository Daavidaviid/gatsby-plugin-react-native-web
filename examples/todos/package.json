{
  "name": "using-typescript",
  "version": "1.0.0",
  "description": "Gatsby example site demonstrating how to use Typescript with Gatsby",
  "author": "fabien0102 <fabien0102@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"yarn start:codegen\" \"yarn start:gatsby\" \"yarn start:typecheck\"",
    "start:gatsby": "yarn apollo:codegen && gatsby develop",
    "start:typecheck": "yarn typecheck --watch",
    "start:codegen": "yarn apollo:codegen --watch",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "typecheck": "tsc --noEmit",
    "lint": "tslint --project .",
    "apollo:codegen": "apollo client:codegen --target=typescript --addTypename --outputFlat src/__apollo_codegen__"
  },
  "dependencies": {
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/node": "^11.11.3",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.2",
    "@types/react-native": "^0.57.40",
    "apollo": "^2.6.1",
    "apollo-boost": "^0.3.1",
    "gatsby": "^2.1.32",
    "gatsby-plugin-react-native-web": "^2.0.0-beta.0",
    "gatsby-plugin-typescript": "^2.0.11",
    "gatsby-plugin-typography": "^2.2.10",
    "isomorphic-fetch": "^2.2.1",
    "query-string": "^6.3.0",
    "react": "^16.4.0",
    "react-apollo": "^2.5.2",
    "react-dom": "^16.4.0",
    "react-native": "^0.55",
    "react-native-web": "^0.11.0",
    "react-typography": "^0.16.13",
    "typography": "^0.16.17",
    "typography-theme-bootstrap": "^0.16.7"
  },
  "devDependencies": {
    "babel-plugin-react-native-web": "^0.11.0",
    "concurrently": "^4.1.0",
    "prettier": "^1.16.4",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333"
  }
}
